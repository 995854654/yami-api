{
  "logger": {
    "path": "/tmp/logs",
    "filename": "access.log",
    "level": "DEBUG",
    "rotation": "1 days",
    "retention": "1 months",
    "format": "{time:YYYY-MM-DD HH:mm:ss.SSS} <level><yellow>{level: <5}</yellow></level> <green>{line}</green> --- [request id:{extra[request_id]}] <cyan>{name}</cyan><cyan>{function}</cyan> : <level>{message}</level>"
  }
}